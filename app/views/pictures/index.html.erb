<ul>
<% @pictures.each do |picture| %>
  <li>
    <%= link_to image_tag(picture.url, :width => 50), picture_url(picture.id) %>
    <nav>
    <%= link_to "Delete", picture_url(picture.id), :method => 'delete', :confirm => "Whoa dude. Are you sure?" %>
    <%= link_to "Edit", edit_picture_url(picture.id) %>
    </nav>
  </li>
<% end %>
</ul>